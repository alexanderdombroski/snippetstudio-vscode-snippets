{
  "Table": {
    "prefix": "table",
    "body": [
      "<table>",
      "    <thead>",
      "        <tr>",
      "            <th>Header 1</th>",
      "            <th>Header 2</th>",
      "        </tr>",
      "    </thead>",
      "    <tbody>",
      "        <tr>",
      "            <td>R1, C1</td>",
      "            <td>R1, C2</td>",
      "        </tr>",
      "        <tr>",
      "            <td>R2, C1</td>",
      "            <td>R2, C2</td>",
      "        </tr>",
      "    </tbody>",
      "</table>"
    ],
    "description": "Generates a table with a thead, and 2x2 tbody"
  },
  "header/main/footer": {
    "prefix": "header",
    "body": [
      "<header>",
      "    <nav>",
      "        ",
      "    </nav>",
      "</header>",
      "<main>",
      "",
      "</main>",
      "<footer>",
      "",
      "</footer>"
    ],
    "description": "Creates a commmon starter structure for a basic webpage"
  },
  "search field": {
    "prefix": "search",
    "body": [
      "<label for=\"searchfield\"></label>",
      "<input list=\"seachlist\" id=\"searchfield\" name=\"searchfield\" type=\"search\" placeholder=\"placeholder\">",
      "<datalist id=\"seachlist\">",
      "    <option value=\"\"></option>",
      "    <option value=\"\"></option>",
      "</datalist>"
    ],
    "description": "Creates an input of type search, a label, and a datalist"
  },
  "Textarea with Label": {
    "prefix": "textarea",
    "body": [
      "<label for=\"${1:id}\">$0</label>",
      "<textarea name=\"${1:id}\" id=\"${1:id}\"${2| , required|}></textarea>"
    ]
  },
  "Input with Label": {
    "prefix": "input",
    "body": [
      "<label for=\"${1:id}\">${1/(.*)/${1:/capitalize}/}:</label>",
      "<input type=\"${3|text,number,email|}\" name=\"${1:id}\" id=\"${1:id}\"${2| , required|}></input>"
    ]
  }
}